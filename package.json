{
  "name": "uny",
  "version": "0.2.5",
  "description": "A modern taste CSS framework",
  "repository": "https://github.com/unys/uny.git",
  "author": "kokushin <mail@kokush.in>",
  "license": "MIT",
  "main": "dist/css/uny.min.css",
  "style": "dist/css/uny.min.css",
  "files": [
    "package.json",
    "CHANGELOG.md",
    "postcss.config.js",
    "README.md",
    ".babelrc",
    ".editorconfig",
    ".eslintrc",
    ".gitattributes",
    ".gitignore",
    "dist",
    "LICENSE",
    "rollup.config.js",
    "src"
  ],
  "scripts": {
    "dev:css": "opener tests/index.html && postcss src/scss/uny.scss -o docs/css/uny.css -w",
    "dev:theme": "opener tests/theme/index.html && postcss src/scss/themes/*.scss -d docs/css -x css -w",
    "dev:js": "opener tests/js/index.html && rollup src/js/uny.js -o docs/js/uny.js -f iife -m inline -c -w",
    "build:css": "NODE_ENV=production postcss src/scss/uny.scss -o dist/css/uny.min.css",
    "build:theme": "NODE_ENV=production postcss src/scss/themes/*.scss -d dist/css -x .min.css",
    "build:js": "NODE_ENV=production rollup src/js/uny.js -o dist/js/uny.min.js -f iife -c",
    "build": "postcss src/scss/uny.scss -o docs/css/uny.css && postcss src/scss/themes/*.scss -d docs/css -x css && npm run build:css && npm run build:theme && npm run build:js",
    "server": "http-server ./docs",
    "deploy": "npm run build && np --no-cleanup"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.3",
    "babel-core": "^6.26.0",
    "babel-preset-es2015-rollup": "^3.0.0",
    "css-mqpacker": "^6.0.1",
    "cssnano": "^3.10.0",
    "http-server": "^0.10.0",
    "np": "^2.16.0",
    "opener": "^1.4.3",
    "postcss": "^6.0.10",
    "postcss-advanced-variables": "^1.2.2",
    "postcss-calc": "^6.0.0",
    "postcss-cli": "^4.1.0",
    "postcss-color-function": "^4.0.0",
    "postcss-easy-import": "^2.1.0",
    "postcss-flexbugs-fixes": "^3.2.0",
    "postcss-nested": "^2.1.2",
    "postcss-sassy-mixins": "^2.1.0",
    "postcss-scss": "^1.0.2",
    "postcss-strip-inline-comments": "^0.1.5",
    "rollup": "^0.50.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-eslint": "^4.0.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^2.0.1"
  },
  "dependencies": {}
}
